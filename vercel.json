{
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "(.*)",
            "destination": "dist/index.js"
        }
    ]
  }